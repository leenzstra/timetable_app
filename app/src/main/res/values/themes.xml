<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Timetable_test" parent="Theme.Material3.Light">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/white_A700</item>
        <item name="colorPrimaryVariant">@color/blue_50</item>
        <item name="colorOnPrimary">@color/white_A700</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/PNRPU_secondary</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
<!--        <item name="android:statusBarColor" >@color/white_A700</item>-->
<!--        <item name="android:windowLightStatusBar" />-->
        <!-- Customize your theme here. -->
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:navigationBarColor">@color/white_A700</item>

        <item name="android:windowLightNavigationBar">true</item>
    </style>

    <style name="Theme.Timetable_test.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Timetable_test.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Timetable_test.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>